<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="bupt.zssdhl.SSHFrame.Bean" >
	<class name="FilePhy" table="FilePhy" lazy="false">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="native"></generator>	
		</id>
		
		<property name="uuName" type="java.lang.String">
			<column name="uuName" length="64"></column>
		</property>
		
		<property name="filePath" type="java.lang.String">
			<column name="filePath" length="128"></column>
		</property>
		
		<property name="MD5" type="java.lang.String">
			<column name="MD5" length="64"></column>
		</property>
		
		<set name="fileInfos" cascade="all">
			<key column="FilePhy_id"></key>
			<one-to-many class="FileInfo"/>
		</set>
	
	</class>

</hibernate-mapping>